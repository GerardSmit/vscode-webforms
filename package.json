{
	"private": true,
	"name": "vscode-webforms",
	"displayName": "WebForms (C#)",
	"description": "Syntax highlighting for WebForms (C#)",
	"repository": {
		"type": "git",
		"url": "https://github.com/GerardSmit/vscode-webforms.git"
	},
	"license": "MIT",
	"version": "1.1.1",
	"publisher": "GerardSmit",
	"author": "GerardSmit",
	"keywords": [
		"webforms",
		"c#"
	],
	"engines": {
		"vscode": "^1.33.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "cs-embedded",
				"aliases": [
					"Embedded C#"
				]
			},
			{
				"id": "webforms",
				"aliases": [
					"WebForms (C#)"
				],
				"firstLine": "^\\s*<%@"
			},
			{
				"id": "html",
				"extensions": [
					".aspx",
					".ascx",
					".asax",
					".master"
				]
			}
		],
		"grammars": [
			{
				"language": "cs-embedded",
				"aliases": [
					"C# Embedded"
				],
				"scopeName": "source.cs-embedded",
				"path": "./syntaxes/csharpEmbedded.tmLanguage.json"
			},
			{
				"language": "webforms",
				"scopeName": "source.webforms",
				"path": "./syntaxes/webforms.tmLanguage.json",
				"injectTo": [
					"text.html"
				]
			}
		]
	}
}
